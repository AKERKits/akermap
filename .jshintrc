{
  "maxerr"        : 50,

  "bitwise"       : true,
  "camelcase"     : true,
  "curly"         : true,
  "eqeqeq"        : true,
  "forin"         : true,
  "freeze"        : true,
  "immed"         : true,
  "indent"        : 4,
  "latedef"       : true,
  "newcap"        : true,
  "noarg"         : true,
  "noempty"       : true,
  "nonbsp"        : true,
  "nonew"         : true,
  "plusplus"      : false,
  "quotmark"      : false,


  "undef"         : true,
  "unused"        : true,
  "strict"        : true,

  "browser"       : true,

  "predef"       : [
    "require",
    "module",
    "angular"
  ]
}
